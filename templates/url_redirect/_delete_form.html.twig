<form method="post" action="{{ path('url_redirect_delete', {'id': url_redirect.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ url_redirect.id) }}">
    <button class="btn btn-secondary">Delete</button>
</form>
